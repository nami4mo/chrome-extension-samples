{
  "name": "TweetDeckColumns",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "tweetdeckの表示列を増やします",

  "permissions":[
    "activeTab"
  ],

  "web_accessible_resources": [
    "style.css",
    "button.png"
  ],

  "background":{
    "scripts":["background.js"]
  },

  "browser_action":{
    "default_icon": "button.png",
    "default_title": "TweetDeckColumns"
  }

}
